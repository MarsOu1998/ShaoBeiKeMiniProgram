/* pages/select/select.wxss */
.select-list{
  width:100%;
}
.select-item{
  width:100%;
}
.select-title{
  width:100%;
  text-align: center;
  padding:20rpx 0;
}
.select-iconList{
  width:100%;
  height:300rpx;
  text-align: center;
  overflow: hidden;
}
.select-iconitem{
  margin:14rpx;
  /* margin-bottom:20rpx; */
  padding:0 30rpx;
  height:70rpx;
  line-height: 70rpx;
  box-sizing: border-box;
  border-radius: 70rpx;
  color:#7ac873;
  border: 1px solid #7ac873;
  display:inline-block;
}

.one-iconList navigator{
  color:#219916;
   border: 1px solid #219916;
}

.two-iconList navigator{
  color:rgb(238, 83, 161);
   border: 1px solid rgb(238, 83, 161);
}

.three-iconList navigator{
  color:rgb(53, 40, 228);
  border: 1px solid rgb(53, 40, 228);
}

.four-iconList navigator{
  color:rgb(96, 36, 165);
  border: 1px solid rgb(96, 36, 165);
}

.five-iconList navigator{
  color:rgb(40, 130, 165);
  border: 1px solid rgb(40, 130, 165);
}